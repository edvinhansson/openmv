    <property name="script">function on_ask(yes)
    if yes then
        message(&quot;Here's some coal!&quot;)
    else
        message(&quot;No coal for you!&quot;)
    end
end

function play()
    if not interacted then
        message(&quot;Hello!&quot;)
        coroutine.yield()
        message(&quot;I'm looking for coal.&quot;)
        coroutine.yield()
        ask(&quot;Would you like some coal? There's plenty to go around.&quot;, &quot;on_ask&quot;)
        interacted = true
    else
        message(&quot;You already talked to me!&quot;)
        coroutine.yield()
    end
end

function on_init()
   interacted = false
end

function on_play()
   co = coroutine.create(play)
end

function on_next_message()
    if coroutine.status(co) ~= &quot;dead&quot; then
        coroutine.resume(co)
    end
end</property>
